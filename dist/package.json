{
    "name": "CustomRaidTimes",
    "version": "1.0.2",
    "main": "src/mod.js",
    "license": "MIT",
    "author": "Dinkadactyl",
    "akiVersion": "3.2.1",
    "scripts": {
        "build": "npm run build:clean && npm run build:distribution && npm run build:dependencies && npm run build:compress",
        "build:clean": "shx rm -f CustomRaidTimes.zip && shx rm -rf ./dist",
        "build:distribution": "copyfiles -e \"./node_modules/**/*.*\" -e \"./dist/**/*.*\" -e \"./types/**/*.*\" -e \"./package-lock.json\" -e \"./tsconfig.json\" -e \"./README.txt\" -e \"./mod.code-workspace\" \"./**/*.*\" ./dist",
        "build:dependencies": "shx mkdir -p ./dist/node_modules/weighted && shx cp -r ./node_modules/weighted ./dist/node_modules",
        "build:compress": "shx mv dist CustomRaidTimes && bestzip CustomRaidTimes.zip CustomRaidTimes && shx mv CustomRaidTimes dist"
    },
    "devDependencies": {
        "@types/node": "^14.15.3",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "copyfiles": "2.4.1",
        "eslint": "7.26.0",
        "shx": "^0.3.4",
        "tsyringe": "4.6.0",
        "typescript": "4.6.4",
        "bestzip": "2.2.1",
        "weighted": "^1.0.0"
    }
}
